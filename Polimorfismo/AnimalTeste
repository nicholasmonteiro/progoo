package semana5;

public class AnimalTeste {

	public static void main(String[] args) 
	{
	
		{
			System.out.println("MAMIFERO");
			
			Mamifero m = new Mamifero();
			
			m.locomover();
			
			m.alimentar();
			
			m.emitirSom();
			
			System.out.println("MMMMMMMMMMMMMMMMMMMMMMMMMMMMMMM");
			
			
			
			System.out.println("REPTIL");
			
			Reptil r = new Reptil();
			
			r.locomover();
			
			r.alimentar();
			
			r.emitirSom();
			
			System.out.println("RRRRRRRRRRRRRRRRRRRR");
			

			
			
			
			System.out.println("AVE");
			Ave a = new Ave();
			a.locomover();
	        a.alimentar();
	        a.emitirSom();
	        a.FazerNinho();
	        System.out.println("AAAAAAAAAAAAAAAAAAA");
	        
	        
	        
	        System.out.println("PEIXE");
			  
           Peixe p = new Peixe();
			
			p.locomover();
			
			p.alimentar();
			
			p.emitirSom();
			
			p.SoltarBolha();
			
			System.out.println("PPPPPPPPPPPPPPPPPPPP");
		}
	}
}

	
