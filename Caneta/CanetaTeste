package aula2;

public class CanetaTester {

	public static void main(String[] args) {
		
		
	Caneta c1 = new Caneta();
	c1.cor="vermelho";
	c1.modelo="bic";
	c1.carga= 90;
	c1.ponta="0.5f";
	c1.tampada= true ;
	c1.rabiscar();
	c1.tampar();
	c1.rabiscar();
	
	Caneta c2 = new Caneta();
	
	c2.cor ="verde";
	
	c2.tampada= false;
	
	c2.modelo= "compactor";

	c2.status();
	c2.rabiscar();

	
	}
}
